{
    "name": "darren-kimberbell/omnipay-paytrace",
    "description": "PayTrace driver for OmniPay PHP Payment Processor",
    "keywords": [
        "omnipay",
        "paytrace"
    ],
    "homepage": "https://github.com/darren-kimberbell/omnipay-paytrace",
    "license": "MIT",
    "authors": [
        {
            "name": "Darren Nay",
            "email": "darren@kimberbell.com",
            "homepage": "https://github.com/darren-kimberbell"
        }
    ],
    "require": {
        "omnipay/common": "^3"
    },
    "require-dev": {
        "omnipay/tests": "^3",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\PayTrace\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.2"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "7.1"
        }
    }
}
